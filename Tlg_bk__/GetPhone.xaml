<Window x:Class="Tlg_bk__.GetPhone"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tlg_bk__"
          mc:Ignorable="d"
        Title="Введите телефонный номер" Height="141.199" Width="459.056" ResizeMode="NoResize" ShowInTaskbar="False" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="Введите номер телефона в формате 81234567890" HorizontalAlignment="Left" Margin="26,10,0,0" VerticalAlignment="Top" Width="315"/>
        <TextBox x:Name="tb_telnumber"  HorizontalAlignment="Left" Height="23" Margin="36,36,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="165" Text=""/>
        <Button x:Name="btn_ok" Content="ОК" HorizontalAlignment="Left" Margin="36,75,0,0" VerticalAlignment="Top" Width="75" Click="btn_ok_Click"/>
        <Button x:Name="btn_cancel" Content="Отмена" HorizontalAlignment="Left" Margin="327,75,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>

